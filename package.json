{
  "name": "atproto-react",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup",
    "version-packages": "changeset version",
    "format": "rome format . --write",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "release": "pnpm build && changeset version && changeset publish"
  },
  "keywords": ["atproto", "bluesky"],
  "author": "Jem Gold <591643+jemgold@users.noreply.github.com> (https://jem.fm/)",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./client": {
      "types": "./client/index.d.ts",
      "require": "./client/index.js",
      "import": "./client/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": ["client", "dist"],
  "dependencies": {
    "@atproto/api": "^0.2.11",
    "jsonwebtoken": "^9.0.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rome": "12.0.0",
    "tsc-files": "^1.1.3",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "yalc": "1.0.0-pre.53"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": ["rome check .", "rome format . --write"],
    "**/*.ts": "tsc-files --noEmit",
    "*.json": ["rome format . --write"]
  }
}
